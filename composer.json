{
    "name": "razorpay/oauth-5-laravel",
    "type": "library",
    "description": "OAuth Service Provider for Laravel 5",
    "keywords": ["OAuth", "carlos-mg89", "laravel", "php"],
    "homepage": "https://github.com/razorpay/oauth-5-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Artdarek",
            "email": "artdarek@gmail.com",
            "role": "Developer"
        },
        {
            "name": "oriceon",
            "email": "oriceon@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "carlos-mg89/oauth": "^0.8"
    },
    "require-dev": {
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0"
    },    
    "require-all": true,
    "extra": {
        "laravel": {
            "providers": [
                "Artdarek\\OAuth\\OAuthServiceProvider"
            ],
            "aliases": {
                "OAuth" : "Artdarek\\OAuth\\Facade\\OAuth"
            }
        }
    },
    "autoload": {    
        "psr-0": {
            "Artdarek\\OAuth": "src/"
        }
    },    
    "minimum-stability": "dev"
}
